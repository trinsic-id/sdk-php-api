{
  "name": "trinsic/api",
  "version": "2.0.0-alpha3",
  "description": "Trinsic API PHP library.",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/trinsic-id/sdk-php-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Trinsic",
      "homepage": "https://trinsic.id"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Trinsic\\Api\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trinsic\\Api\\Test\\": "test/"
    }
  }
}
